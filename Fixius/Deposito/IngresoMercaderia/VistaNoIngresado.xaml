<UserControl x:Class="Inteldev.Fixius.Deposito.IngresoMercaderia.VistaItemNoIngresado"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c="clr-namespace:Inteldev.Core.Presentacion.Controles;assembly=Inteldev.Core.Presentacion"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
        <c:ItemFormularioMiniBusca Etiqueta="Artículo" Presentador="{Binding PresentadorArticulo, Mode= TwoWay, UpdateSourceTrigger=PropertyChanged}"></c:ItemFormularioMiniBusca>
        <c:ItemFormularioMiniBusca Etiqueta="Motivo" Presentador="{Binding PresentadorMotivo, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"></c:ItemFormularioMiniBusca>
        <c:ItemFormularioNumeroEntero Etiqueta="Cantiad" Valor="{Binding Bultos, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"></c:ItemFormularioNumeroEntero>
    </StackPanel>
</UserControl>
