<UserControl x:Class="Inteldev.Fixius.Proveedores.Compras.ListaDePrecios.VistaListaProveedorNuevo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:controles="clr-namespace:Inteldev.Core.Presentacion.Controles;assembly=Inteldev.Core.Presentacion"
             xmlns:presentadores="clr-namespace:Inteldev.Fixius.Presentadores"
             Loaded="UserControl_Loaded">
    
    <StackPanel Margin="10">
        <TextBlock Text="Seleccione un Proveedor" Padding="0,10,10,20"  FontSize="22"></TextBlock>
        <controles:ItemFormularioMiniBusca x:Name="mbProveedor"
                                           Etiqueta="Proveedor"                                           
                                           Presentador="{Binding PresentadorMiniBuscaProveedor}"
                                           Entidad="{Binding Path=Modelo.Proveedor}"
            />
    </StackPanel>
</UserControl>
